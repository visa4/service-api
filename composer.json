{
    "name": "visa4/service-api",
    "description": "A set of utilities aimed at consuming HTTP API services",
    "homepage": "http://github.com/matryoshka-model/service-api",
    "license": "BSD-2-Clause",
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-http": "~2.1",
        "zendframework/zend-json": "~2.1",
        "zendframework/zend-stdlib": "~2.1",
        "zendframework/zendxml": "~1.0-dev",
        "zfcampus/zf-api-problem": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "satooshi/php-coveralls": "dev-master",
        "zendframework/zend-servicemanager": "~2.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev",
            "dev-develop": "0.4.x-dev"
        }
    },
    "authors": [
        {
            "name": "Leonardo Grasso",
            "email": "me@leonardograsso.com",
            "homepage": "http://github.com/leogr"
        },
        {
            "name": "Leo Di Donato",
            "email": "leodidonato@gmail.com",
            "homepage": "http://github.com/leodido"
        },
        {
            "name": "Antonio Visalli",
            "email": "antonino.visalli@gmail.com",
            "homepage": "http://github.com/visa4"
        }
    ],
    "autoload": {
        "psr-4": {
        	"Matryoshka\\Service\\Api\\": "library/",
        	"MatryoshkaServiceApiTest\\": "tests/"
        }
    },
    "keywords": [
        "matryoshka",
        "rest",
        "restful",
        "rpc",
        "API",
        "web services",
        "http",
        "RPC",
        "RESTful"
    ],
    "suggest": {
        "matryoshka-model/matryoshka": "A lightweight framework that provides a standard and easy way to implement a model service layer",
        "matryoshka-model/zf2-matryoshka-module": "ZF2 module for matryoshka library",
        "matryoshka-model/rest-wrapper": "Matryoshka wrapper aimed at creating restful API clients"
    }
}
